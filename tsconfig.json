{
  "include": ["./src/**/*", "./gatsby-*.ts"],
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "lib": ["dom", "es2017"],
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "jsxImportSource": "@emotion/react",
    "resolveJsonModule": true
  }
}
